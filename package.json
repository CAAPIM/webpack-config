{
  "name": "webpack-config-ca",
  "version": "1.0.0",
  "description": "Configuration for Webpack projects at CA",
  "main": "dist/index.js",
  "repository": "https://github-isl-01.ca.com/APIM-WEB/webpack-config-ca",
  "keywords": [
    "webpack",
    "config"
  ],
  "author": "Bevan Hunt <bevan.hunt@ca.com>",
  "license": "MIT",
  "dependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer": "^6.5.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-ca": "git+ssh://git@github-isl-01.ca.com:APIM-WEB/babel-preset-ca.git",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-runtime": "^6.18.0",
    "baggage-loader": "^0.2.4",
    "browser-sync": "^2.18.2",
    "browser-sync-webpack-plugin": "^1.1.3",
    "clean-webpack-plugin": "^0.1.13",
    "css-loader": "^0.25.0",
    "dotenv": "^2.0.0",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.36.0",
    "flow-status-webpack-plugin": "^0.1.7",
    "html-loader": "^0.4.4",
    "image-webpack-loader": "^3.0.0",
    "imagemin": "^5.2.2",
    "json-loader": "^0.5.4",
    "merge": "^1.2.0",
    "node-sass": "^3.10.1",
    "object-path": "^0.11.2",
    "postcss-loader": "^1.1.0",
    "sass-loader": "^4.0.2",
    "source-map-loader": "^0.1.5",
    "stats-webpack-plugin": "^0.4.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack-config": "^6.1.3",
    "webpack-node-externals": "^1.5.4"
  },
  "peerDependencies": {
    "webpack": "*"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.11.1",
    "eslint-config-ca": "git+ssh://git@github.com:CAAPIM/eslint-config-ca.git",
    "jest": "^17.0.3",
    "pre-commit": "^1.1.3",
    "react": "^15.4.1",
    "semantic-release": "^6.3.2",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "husky": "^0.11.9",
    "validate-commit-msg": "^2.8.2"
  },
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint ./src/ --ext .js",
    "flow": "flow",
    "build": "babel src --out-dir dist",
    "install": "npm run build",
    "test:acceptance": "cd tests/acceptance; webpack --bail; NODE_ENV=production webpack --bail",
    "test:unit": "jest",
    "test": "npm run build; npm run test:acceptance; npm run test:unit",
    "release": "semantic-release pre && npm publish && semantic-release post",
    "commitmsg": "validate-commit-msg"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    },
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
